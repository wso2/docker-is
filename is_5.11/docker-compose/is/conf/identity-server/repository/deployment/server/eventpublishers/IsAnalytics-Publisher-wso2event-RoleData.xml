<eventPublisher xmlns="http://wso2.org/carbon/eventpublisher" name="IsAnalytics-Publisher-wso2event-RoleData" statistics="disable" trace="disable">
  <from streamName="org.wso2.is.analytics.stream.OverallRoleData" version="1.0.0"/>
  <mapping customMapping="disable" type="wso2event"/>
  <to eventAdapterType="wso2event">
    <property name="username">admin</property>
    <property name="protocol">thrift</property>
    <property name="publishingMode">non-blocking</property>
    <property name="publishTimeout">0</property>
    <property name="receiverURL">tcp://identity-server-analytics-worker:7612</property>
    <property encrypted="false" name="password">admin</property>
  </to>
</eventPublisher>
